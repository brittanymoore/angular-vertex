{
    "name": "vertex",
    "version": "0.0.1",
    "description": "seed project for angular 2",
    "scripts": {
        "install:verbose": "npm install --loglevel verbose",
        "compile:aot": "./node_modules/.bin/ngc -p ./tsconfig.aot.json",
        "clean:dev": "rimraf dev/*.js dev/*.map",
        "clean:prod": "rimraf dist/*.js",
        "clean:prod:aot": "rimraf compiled-aot && rimraf dist-aot/*.js dist-aot/*.map",
        "clean:prod:sp": "rimraf dist-sp/*.js",
        "clean:": "npm run clean:dev",
        "build:dev": "npm run clean:dev && webpack --progress --config ./config/webpack.dev.js",
        "build:prod": "npm run clean:prod && webpack --progress --config ./config/webpack.prod.js",
        "build:prod:aot": "npm run clean:prod:aot && webpack --progress --config ./config/webpack.prod.aot.js",
        "build:prod:sp": "npm run clean:prod:sp && webpack --progress --config ./config/webpack.prod.sp.js",
        "build": "npm run build:dev",
        "server:dev": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress --port 3000 --content-base dev",
        "server:prod": "webpack-dev-server --config ./config/webpack.prod.js --inline --progress --port 3000 --content-base dist",
        "server:prod:aot": "webpack-dev-server --config ./config/webpack.prod.aot.js --inline --progress --port 3000 --content-base dist-aot",
        "server": "npm run server:dev",
        "start:dev": "npm run clean:dev && npm run server:dev",
        "start:prod": "npm run clean:prod && npm run server:prod",
        "start:prod:aot": "npm run clean:prod:aot && npm run server:prod:aot",
        "start": "npm run start:dev",
        "karma:start": "karma start ./config/karma.conf.js ",
        "test:unit": "npm run karma:start",
        "webdriver-manager": "webdriver-manager",
        "webdriver:update": "npm run webdriver-manager update",
        "webdriver:start": "npm run webdriver-manager start",
        "protractor": "protractor ./config/protractor.conf.js",
        "test:e2e": "npm run protractor",
        "typedoc": "typedoc --options ./config/typedoc.json --tsconfig ./config/typedoc.tsconfig.json ./src"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "~2.2.1",
        "@angular/compiler": "~2.2.1",
        "@angular/compiler-cli": "~2.2.1",
        "@angular/core": "~2.2.1",
        "@angular/platform-browser": "~2.2.1",
        "@angular/platform-browser-dynamic": "~2.2.1",
        "@angular/platform-server": "~2.2.1",
        "@angular/forms": "~2.2.0",
        "@angular/http": "~2.2.0",
        "@angular/router": "3.2.1",
        "angular-in-memory-web-api": "~0.1.15",
        "core-js": "^2.4.1",
        "ie-shim": "^0.1.0",
        "rxjs": "5.0.0-beta.12",
        "zone.js": "~0.6.26"
    },
    "devDependencies": {
        "@types/node": "^6.0.48",
        "@types/jasmine": "^2.2.34",
        "@types/core-js": "^0.9.35",
        "awesome-typescript-loader": "^2.2.4",
        "angular2-router-loader": "^0.3.4",
        "angular2-template-loader": "^0.6.0",
        "typescript": "~2.0.10",
        "css-loader": "^0.25.0",
        "raw-loader": "^0.5.1",
        "to-string-loader": "^1.1.4",
        "webpack": "2.1.0-beta.27",
        "webpack-dev-server": "2.1.0-beta.11",
        "webpack-merge": "^0.15.0",
        "html-webpack-plugin": "^2.24.1",
        "@ngtools/webpack": "^1.1.6",
        "typedoc": "^0.5.1",
        "rimraf": "^2.5.4",
        "karma": "^1.2.0",
        "karma-jasmine": "^1.0.2",
        "karma-chrome-launcher": "^2.0.0",
        "karma-webpack": "1.8.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "^0.0.26",
        "jasmine": "^2.5.2",
        "protractor": "^4.0.10",
        "ts-node": "^1.7.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brittanymoore/vertex.git"
    }
}